{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "never"],
        "quotes": ["error", "double"],
        "linebreak-style": [
            "error", 
            "windows"
        ], 
        "no-param-reassign": ["error", { "porps": false }]
    }
}
